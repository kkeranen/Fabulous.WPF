<Window x:Class="Sudoku.CS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sudoku.CS"
        xmlns:tiles="clr-namespace:Sudoku.CS.Tiles"
        xmlns:regions="clr-namespace:Sudoku.CS.Regions"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <regions:RegionViewModel/>
    </Window.DataContext>
    <Window.Resources>
        <DataTemplate DataType="{x:Type regions:RegionViewModel}">
            <regions:RegionView />
        </DataTemplate>
    </Window.Resources>
    <Grid>
        <ContentControl Content="{Binding }" />
</Grid>
</Window>
